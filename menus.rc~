+ ""		Nop
Test (x wine)		+ "Wine"			Popup Wine
Test (x /opt/cxoffice/bin/cxsetup)\
			+ "CrossOver Linux"		Popup CxOffice
Test (f $[HOME]/.Xnest.sh) Test (x Xnest)\
			+ "Xnest"			Exec ~/.fvwm/Xnest.sh
+ ""		Nop
Test (x xfce4-appfinder)\
			+ "Localizador..."		Exec xfce4-appfinder --disable-server
+ ""		Nop
Test (x /sbin/yast2) 	+ "YaST2 (GTK)"			Exec /usr/bin/xdg-su -c '/sbin/yast2 --gtk'
Test (x /sbin/yast2) 	+ "YaST2 (Qt)"			Exec /usr/bin/xdg-su -c '/sbin/yast2 --qt'
Test (x mcc)	 	+ "Mandriva Control Center"	Exec mcc
+ ""		Nop
Test (x gdmflexiserver) + "Nova sessão"			Exec gdmflexiserver
Test (x gdmflexiserver) + "Nova sessão em uma janela"	Exec gdmflexiserver --xnest

