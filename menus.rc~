#=====================================================================#
echo menus.rc: Processando menus do ambiente...

#
# Menus
#

# =====================================================================
# Menu Principal e Submenus
#
# Os menus declarados abaixo fazem parte do conjunto principal do
# ambiente, sendo utilizados para a abertura de programas de 
# aplicativos, invocação de módulos e outras funções triviais.
# =====================================================================

#
# Menus em XDG para o openSUSE
# Recurso adicionado em: Seg Jan 28 02:35:07 BRST 2013
#
Test (x xdg_menu) \
	PipeRead 'xdg_menu --charset UTF-8 --format fvwm2'

# Menu Principal
#
DestroyMenu Utilities
AddToMenu Utilities
#"AddToMenu Utilities@side.fvwm2.xpm@^black^"
+ "Menu Principal" Title
+ "" TearMenuOff
+ "Terminal%$[Fvwm_Icon_Theme]/32x32/apps/gnome-term-linux.png%" \
				Exec xfce4-terminal
+ "Monitor de Processos%$[Fvwm_Icon_Theme]/32x32/apps/gnome-monitor.png%" \
				Exec xterm -geometry 140x50 -bg black -fg lightgray \
				-font 7x14 -T htop -n htop -e htop
+ "Calculadora%$[Fvwm_Icon_Theme]/32x32/apps/gnome-calculator.png%" \
				Exec galculator
+ "Lente de Aumento%$[Fvwm_Icon_Theme]/32x32/apps/gnome-searchtool.png%" \
				Exec xmag
+ "Páginas de Manual%$[Fvwm_Icon_Theme]/32x32/apps/gnome-help.png%" \
				Exec xman
+ ""		Nop
Test (x xdg_menu) \
	+ "openSUSE (&XDG)%$[Fvwm_Icon_Theme]/32x32/places/start-here-branding.png%" \
				Popup xdg_menu
+ "&Aplicativos%$[Fvwm_Icon_Theme]/32x32/apps/programs.png%" \
				Popup Applications
+ "&Utilitários%$[Fvwm_Icon_Theme]/32x32/apps/accessories-calculator.png%" \
				Popup Accessories
+ "&Gráficos%$[Fvwm_Icon_Theme]/32x32/apps/gnome-graphics.png%" \
				Popup Graphics
+ "&Desenvolvimento%$[Fvwm_Icon_Theme]/32x32/apps/icon-install-software.png%" \
				Popup Develop
+ "C&ientífico%$[Fvwm_Icon_Theme]/32x32/apps/accessories-dictionary.png%" \
				Popup Science
+ "&Produtividade%$[Fvwm_Icon_Theme]/32x32/apps/gnome-applications.png%" \
				Popup Office
+ "&Conectividade%$[Fvwm_Icon_Theme]/32x32/apps/applications-internet.png%" \
				Popup Comm
+ "&Multimídia%$[Fvwm_Icon_Theme]/32x32/apps/gnome-multimedia.png%" \
				Popup Multimedia
+ "Ar&quivos%$[Fvwm_Icon_Theme]/32x32/apps/file-manager.png%" \
				Popup Filemanagers
+ "&Jogos%$[Fvwm_Icon_Theme]/32x32/apps/applications-games.png%" \
				Popup Games
+ "Sh&ells%$[Fvwm_Icon_Theme]/32x32/apps/gnome-terminal.png%" \
				Popup Shells
+ ""		Nop
+ "Módulo&s%$[Fvwm_Icon_Theme]/32x32/apps/gnome-devel.png%" \
				Popup Module-Popup
+ ""		Nop
+ "Executa&r...%$[Fvwm_Icon_Theme]/32x32/apps/gnome-run.png%" \
				Exec xfce4-appfinder -c --disable-server
+ "Redesen&har Tela%$[Fvwm_Icon_Theme]/32x32/apps/gnome-settings-background.png%" \
				Refresh
+ "&Organizar Ícones%$[Fvwm_Icon_Theme]/32x32/apps/gnome-window-manager.png%" \
				All (CurrentPage, Iconic) PlaceAgain Icon
+ ""		Nop
+ "Sair%$[Fvwm_Icon_Theme]/32x32/actions/exit.png%" \
				Popup Quit-Verify

# Aplicativos
#
DestroyMenu Applications
AddToMenu Applications
+ "" TearMenuOff
Test (x VirtualBox) 	+ "Oracle VM VirtualBox Manager" \
							Exec XIM_PROGRAM="" QT_IM_MODULE="" XMODIFIERS="" VirtualBox
Test (x vmware)		+ "VMware Server"		Exec vmware
